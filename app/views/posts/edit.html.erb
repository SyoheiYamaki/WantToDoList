<h2>編集</h2>

<%= form_for @post, url: post_path(@post) do |f|  %>
	<p><%= f.text_field :title %></p>
	<p><%= f.text_area :content %></p>
	<p><%= f.submit  %></p>
<% end %>


<%= link_to "削除" , post_path(@post.id ), method: :delete, data:{confirm: "本当に削除してもよろしいですか？"} %>
<%= link_to "戻る", posts_path %>